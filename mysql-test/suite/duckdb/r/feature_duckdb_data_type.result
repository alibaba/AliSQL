String data type test
check data consistency
CREATE TABLE t1 (
id int PRIMARY KEY,
col1 char(100),
col2 varchar(100),
col3 binary(4),
col4 varbinary(100),
col5 tinyblob,
col6 blob,
col7 mediumblob,
col8 longblob,
col9 tinytext,
col10 text,
col11 mediumtext,
col12 longtext,
col13 json,
col14 enum('v1', 'v2', 'v3'),
col15 set('v1', 'v2', 'v3')
) ENGINE = DuckDB;
insert into t1 values (1, 'aaaa', 'aaaa', 'aaaa', 'aaaa', 'aaaa', 'aaaa', 'aaaa', 'aaaa', 'aaaa', 'aaaa', 'aaaa', 'aaaa', '{"id": 1, "value":"aaaa"}', 1, 1);
insert into t1 values (2, 'bbbb', 'bbbb', 'bbbb', 'bbbb', 'bbbb', 'bbbb', 'bbbb', 'bbbb', 'bbbb', 'bbbb', 'bbbb', 'bbbb', '{"id": 1, "value":"aaaa"}', 'v2', 'v2');
