insert into t1 values (4, repeat('d', 100), repeat('d', 100), repeat('d', 4), repeat('d', 100), 
repeat('d', 255), repeat('d', 65535), repeat('d', 16777215), repeat('d', 60000000),
repeat('d', 255), repeat('d', 65535), repeat('d', 16777215), repeat('d', 60000000),
'{}', 3, 'v3');
